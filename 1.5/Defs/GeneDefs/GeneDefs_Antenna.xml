	<GeneDef ParentName="Outland_GeneAntennaBase">
		<defName>Outland_Antenna_Feather</defName>
		<label>antenna (feather)</label>
		<description>Carriers of this gene will grow a pair of moth-like antenna from their head.</description>
		<iconPath>Outland/Genes/Antenna/Feather</iconPath>
		<displayOrderInCategory>7901</displayOrderInCategory>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Outland/Genes/Antenna/Feather</texPath>
				<colorType>Hair</colorType>
				<parentTagDef>Head</parentTagDef>
				<drawData>
					<defaultData>
						<layer>80</layer>
					</defaultData>
					<dataNorth>
						<layer>10</layer>
					</dataNorth>
				</drawData>
				<useSkinShader>false</useSkinShader>
			</li>
		</renderNodeProperties>
	</GeneDef>

