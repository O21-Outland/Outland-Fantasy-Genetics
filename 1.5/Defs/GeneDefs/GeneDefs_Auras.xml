<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<GeneDef Name="Outland_AuraGeneBase" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<randomChosen>true</randomChosen>
		<geneClass>TabulaRasa.Gene_Effecter</geneClass>
		<displayCategory>Outland_Auras</displayCategory>
		<displayOrderInCategory>100</displayOrderInCategory>
		<canGenerateInGeneSet>false</canGenerateInGeneSet>
		<customEffectDescriptions>
			<li>Visual effect only.</li>
		</customEffectDescriptions>
		<exclusionTags>
			<li>Aura</li>
		</exclusionTags>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Outland_AuraGeneBase">
		<defName>Outland_Aura_FireAspect</defName>
		<label>aura (fire)</label>
		<description>Carriers of this gene produce a visible aura, with small fire element glyphs appearing occasionally.</description>
		<iconPath>Outland/Genes/Auras/Gene_FireAspect</iconPath>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_GeneEffecter">
				<effecter>Outland_Effecter_FireAspect</effecter>
			</li>
		</modExtensions>
	</GeneDef>

