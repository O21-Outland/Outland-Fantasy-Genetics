	<GeneDef ParentName="Outland_GeneHeadboneCrownBase">
		<defName>Outland_Headbone_CrownArcuate</defName>
		<label>crown (arcuate)</label>
		<description>Carriers of this gene grow a crown of horns from their head.</description>
		<iconPath>Outland/Genes/Crowns/Arcuate</iconPath>
		<displayOrderInCategory>8000</displayOrderInCategory>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Outland/Genes/Crowns/Arcuate</texPath>
				<colorType>Skin</colorType>
				<parentTagDef>Head</parentTagDef>
				<drawData>
					<defaultData>
						<layer>51</layer>
					</defaultData>
				</drawData>
			</li>
		</renderNodeProperties>
	</GeneDef>

