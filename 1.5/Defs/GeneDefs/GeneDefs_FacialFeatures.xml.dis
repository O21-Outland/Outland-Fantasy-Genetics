<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- ============================== Eye Visuals ============================== -->

	<GeneDef Name="Outland_GeneEyeVisualBase" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<displayCategory>Outland_FacialFeatures</displayCategory>
		<displayOrderInCategory>7900</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<exclusionTags>
			<li>EyeVisuals</li>
		</exclusionTags>
		<graphicData>
			<drawLoc>HeadMiddle</drawLoc>
			<colorType>Skin</colorType>
			<visibleNorth>false</visibleNorth>
			<useSkinShader>false</useSkinShader>
			<layer>PostSkin</layer>
		</graphicData>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneEyeVisualBase">
		<defName>Outland_EyeVisual_Recessed</defName>
		<label>recessed eyes</label>
		<description>Carriers of this gene have recessed eye sockets, making the area around their eyes much darker than usual.</description>
		<iconPath>Outland/GeneIcons/Gene_RecessedEyes</iconPath>
		<displayOrderInCategory>7801</displayOrderInCategory>
		<graphicData>
			<graphicPath>Outland/Genes/Eyes/Male_RecessedEyes</graphicPath>
			<graphicPathFemale>Outland/Genes/Eyes/Female_RecessedEyes</graphicPathFemale>
		</graphicData>
	</GeneDef>

	<!-- ============================== Facial Features ============================== -->

	<GeneDef Name="Outland_GeneFaceFeatureBase" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<displayCategory>Outland_FacialFeatures</displayCategory>
		<displayOrderInCategory>7900</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<exclusionTags>
			<li>FacialFeatures</li>
		</exclusionTags>
		<graphicData>
			<layer>PostSkin</layer>
			<colorType>Skin</colorType>
			<visibleNorth>false</visibleNorth>
			<drawOffset>(0, 0.002, 0)</drawOffset>
		</graphicData>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneFaceFeatureBase">
		<defName>Outland_FaceTendrils</defName>
		<label>facial tendrils</label>
		<description>Carriers of this gene grow tendril like growths on their face.</description>
		<iconPath>Outland/Genes/Face/FaceTendrils</iconPath>
		<displayOrderInCategory>500</displayOrderInCategory>
		<biostatCpx>0</biostatCpx>
		<graphicData>
			<graphicPaths>
				<li>Outland/Genes/Face/FaceTendrils1</li>
				<li>Outland/Genes/Face/FaceTendrils2</li>
				<li>Outland/Genes/Face/FaceTendrils3</li>
				<li>Outland/Genes/Face/FaceTendrils4</li>
				<li>Outland/Genes/Face/FaceTendrils5</li>
				<li>Outland/Genes/Face/FaceTendrils6</li>
				<li>Outland/Genes/Face/FaceTendrils7</li>
				<li>Outland/Genes/Face/FaceTendrils8</li>
				<li>Outland/Genes/Face/FaceTendrils9</li>
				<li>Outland/Genes/Face/FaceTendrils10</li>
				<li>Outland/Genes/Face/FaceTendrils11</li>
				<li>Outland/Genes/Face/FaceTendrils12</li>
				<li>Outland/Genes/Face/FaceTendrils13</li>
			</graphicPaths>
			<visibleNorth>false</visibleNorth>
			<layer>PostSkin</layer>
			<colorType>Skin</colorType>
		</graphicData>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneFaceFeatureBase">
		<defName>Outland_Beak_Hooked</defName>
		<label>beak (hooked)</label>
		<description>Carriers of this gene grow a beak in place of a normal mouth.</description>
		<iconPath>Outland/Genes/Face/Beak</iconPath>
		<displayOrderInCategory>501</displayOrderInCategory>
		<biostatCpx>0</biostatCpx>
		<graphicData>
			<graphicPath>Outland/Genes/Face/Beak</graphicPath>
			<visibleNorth>false</visibleNorth>
			<layer>PostSkin</layer>
			<colorType>Custom</colorType>
		</graphicData>
	</GeneDef>

