	<GeneDef ParentName="Outland_GeneTailBase">
		<defName>Outland_Tail_Pointed</defName>
		<label>tail (pointed)</label>
		<description>Carriers of this gene grow a prehensile tail which increases manipulation slightly.</description>
		<iconPath>Outland/Genes/Tails/Common/Pointed</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<displayOrderInCategory>7900</displayOrderInCategory>
		<capMods>
			<li>
				<capacity>Manipulation</capacity>
				<offset>0.05</offset>
			</li>
		</capMods>
		<biostatMet>-1</biostatMet>
		<renderNodeProperties>
			<li>
				<workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
				<texPath>Outland/Genes/Tails/Common/Pointed</texPath>
