		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<randomChosen>true</randomChosen>
		<exclusionTags>
			<li>Feet</li>
		</exclusionTags>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneFeetBase">
		<defName>Outland_HoovedFeet</defName>
		<label>feet (hoof)</label>
		<description>Carriers of this gene have hooves in place of feet, allowing them greater speed.</description>
		<iconPath>Outland/GeneIcons/Gene_HoovedFeet</iconPath>
		<displayOrderInCategory>7901</displayOrderInCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<capMods>
			<li>
				<capacity>Moving</capacity>
				<postFactor>1.1</postFactor>
			</li>
		</capMods>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>hoof</symbol>
				</li>
				<li>
					<symbol>cow</symbol>
				</li>
				<li>
					<symbol>bull</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>hoof</symbol>
				</li>
				<li>
					<symbol>toe</symbol>
