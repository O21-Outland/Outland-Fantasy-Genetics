<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<GeneDef ParentName="GeneJawBase" Name="Outland_GeneHeadBase" Abstract="True">
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneHeadBase">
		<defName>Outland_SvelteHead</defName>
		<label>svelte head</label>
		<description>Carriers of this gene have an elegant and slender appearance in their face and head.</description>
		<iconPath>Outland/GeneIcons/Gene_SvelteHead</iconPath>
		<forcedHeadTypes>
			<li>Outland_Male_Svelte</li>
			<li>Outland_Female_Svelte</li>
		</forcedHeadTypes>
		<statOffsets>
			<PawnBeauty>1</PawnBeauty>
		</statOffsets>
		<displayOrderInCategory>9701</displayOrderInCategory>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>svelte</symbol>
				</li>
				<li>
					<symbol>elegant</symbol>
				</li>
				<li>
					<symbol>slender</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneHeadBase">
		<defName>Outland_MothHead</defName>
		<label>moth head</label>
		<description>Carriers of this gene have a moth-like appearance in their face and head.\n\nNote: This head type has sexual dimorphism, meaning male and female heads differ. It's not a bug, it's intended.</description>
		<iconPath>Outland/GeneIcons/Gene_MothHead</iconPath>
		<forcedHeadTypes>
			<li>Outland_Male_Moth</li>
			<li>Outland_Female_Moth</li>
		</forcedHeadTypes>
		<displayOrderInCategory>9702</displayOrderInCategory>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>moth</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneHeadBase">
		<defName>Outland_InsectHead</defName>
		<label>insect head</label>
		<description>Carriers of this gene have a insect-like appearance in their face and head.</description>
		<iconPath>Outland/GeneIcons/Gene_InsectHead</iconPath>
		<forcedHeadTypes>
			<li>Outland_Insect</li>
		</forcedHeadTypes>
		<displayOrderInCategory>9703</displayOrderInCategory>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>insect</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneHeadBase">
		<defName>Outland_TermiteHead</defName>
		<label>termite head</label>
		<description>Carriers of this gene have a termite-like appearance in their face and head, while they appear to lack eyes they do in fact have them, they're just covered in a layer of skin making it difficult for them to see out.</description>
		<iconPath>Outland/GeneIcons/Gene_TermiteHead</iconPath>
		<biostatCpx>1</biostatCpx>
		<biostatMet>2</biostatMet>
		<forcedHeadTypes>
			<li>Outland_Termite</li>
		</forcedHeadTypes>
		<displayOrderInCategory>9704</displayOrderInCategory>
		<capMods>
			<li>
				<capacity>Sight</capacity>
				<offset>-0.5</offset>
			</li>
		</capMods>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>insect</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Outland_GeneHeadBase">
		<defName>Outland_DraconicHead</defName>
		<label>draconic head</label>
		<description>Carriers of this gene have a muzzled head shape, reminiscent of a dragons head.</description>
		<iconPath>Outland/GeneIcons/Gene_DraconicHead</iconPath>
		<forcedHeadTypes>
			<li>Outland_Draconic</li>
		</forcedHeadTypes>
		<displayOrderInCategory>9705</displayOrderInCategory>
		<beardTagFilter>
			<whitelist>true</whitelist>
			<tags>
				<li>NoBeard</li>
			</tags>
		</beardTagFilter>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>dragon</symbol>
				</li>
				<li>
					<symbol>wyvern</symbol>
				</li>
				<li>
					<symbol>drake</symbol>
				</li>
				<li>
					<symbol>kobold</symbol>
				</li>
				<li>
					<symbol>devil</symbol>
				</li>
				<li>
					<symbol>demon</symbol>
				</li>
				<li>
					<symbol>scamp</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>

