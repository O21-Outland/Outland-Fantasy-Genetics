	<!-- ========== Blood: Red ========= -->

	<GeneDef ParentName="Outland_BloodTypeBase">
		<defName>Outland_Blood_Red</defName>
		<label>red blood</label>
		<labelShortAdj>red-blooded</labelShortAdj>
		<description>Carriers of this gene have red blood.</description>
		<iconColor>(173, 37, 37)</iconColor>
		<displayOrderInCategory>7901</displayOrderInCategory>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<customBloodThingDef>Outland_Filth_BloodRed</customBloodThingDef>
				<customBloodIcon>Outland/Medical/BleedingRed</customBloodIcon>
