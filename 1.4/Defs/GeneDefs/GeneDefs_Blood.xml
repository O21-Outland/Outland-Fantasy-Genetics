				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<ThingDef ParentName="BaseFilth">
		<defName>Outland_Filth_BloodRed</defName>
		<label>red blood</label>
		<statBases>
			<Beauty>-30</Beauty>
			<Cleanliness>-10</Cleanliness>
		</statBases>
		<graphicData>
			<texPath>Things/Filth/Spatter</texPath>
			<color>(173, 37, 37, 180)</color>
		</graphicData>
		<filth>
			<ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
			<disappearsInDays>35~40</disappearsInDays>
			<rainWashes>true</rainWashes>
