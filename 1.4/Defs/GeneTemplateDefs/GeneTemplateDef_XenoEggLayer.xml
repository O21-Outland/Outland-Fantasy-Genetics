<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<OutlandGenes.XenoEggGeneTemplateDef>
		<defName>Outland_XenoEggLayer</defName>
		<label>{0} egg layer</label>
		<labelShortAdj>{0} egg layer</labelShortAdj>
		<description>Carriers of this gene reproduce asexually by laying eggs that hatch a pawn that is specifically an {0} xenotype. They can only do so once a year.</description>
		<geneClass>OutlandGenes.Gene_XenoEggLayer</geneClass>
		<minAgeActive>18</minAgeActive>
		<ignoreIllegalLabelCharacterConfigError>true</ignoreIllegalLabelCharacterConfigError>
		<displayCategory>Outland_XenotypeEggs</displayCategory>
		<displayOrderOffset>3</displayOrderOffset>
		<biostatCpx>1</biostatCpx>
		<biostatMet>0</biostatMet>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<hediffToWholeBody>Outland_EggPouch</hediffToWholeBody>
				<backgroundPathEndogenes>Outland/GeneIcons/EndogeneBG</backgroundPathEndogenes>
				<backgroundPathXenogenes>Outland/GeneIcons/XenogeneBG</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</OutlandGenes.XenoEggGeneTemplateDef>

</Defs>